{
    "dataset_reader": {
        "type": "scicite_datasetreader",
        "multilabel": "false",
        "use_sparse_lexicon_features": "false",
        "with_elmo": "true"
    },
    "iterator": {
        "type": "bucket",
        "batch_size": 16,
        "sorting_keys": [
            [
                "citation_text",
                "num_tokens"
            ]
        ]
    },
    "model": {
        "type": "scaffold_bilstm_attention_classifier",
        "citation_text_encoder": {
            "type": "gru",
            "bidirectional": true,
            "dropout": 0.3,
            "hidden_size": 100,
            "input_size": 1324,
            "num_layers": 2
        },
        "classifier_feedforward": {
            "activations": [
                "linear",
                "linear"
            ],
            "dropout": [
                0,
                0
            ],
            "hidden_dims": [
                20,
                3
            ],
            "input_dim": 200,
            "num_layers": 2
        },
        "classifier_feedforward_2": {
            "activations": [
                "linear",
                "linear"
            ],
            "dropout": [
                0,
                0
            ],
            "hidden_dims": [
                20,
                5
            ],
            "input_dim": 200,
            "num_layers": 2
        },
        "classifier_feedforward_3": {
            "activations": [
                "linear",
                "linear"
            ],
            "dropout": [
                0,
                0
            ],
            "hidden_dims": [
                20,
                2
            ],
            "input_dim": 200,
            "num_layers": 2
        },
        "data_format": "scicite_flat_jsonlines",
        "elmo_text_field_embedder": {
            "elmo": {
                "type": "elmo_token_embedder",
                "do_layer_norm": "true",
                "dropout": 0.5,
                "options_file": "https://allennlp.s3.amazonaws.com/models/elmo/2x4096_512_2048cnn_2xhighway/elmo_2x4096_512_2048cnn_2xhighway_options.json",
                "weight_file": "https://allennlp.s3.amazonaws.com/models/elmo/2x4096_512_2048cnn_2xhighway/elmo_2x4096_512_2048cnn_2xhighway_weights.hdf5"
            },
            "tokens": {
                "type": "embedding",
                "embedding_dim": 300,
                "pretrained_file": "/home/kanhon/Desktop/kanhon/NUS Computing/CS4248/github_dir/cs4248/scaffold/Project/sci-cite/scicite/pretrained_weights/GoogleNews-vectors-negative300.txt",
                "trainable": "false"
            }
        },
        "lexicon_embedder": {
            "embedding_dim": 100,
            "pretrained_file": "https://s3-us-west-2.amazonaws.com/allennlp/datasets/glove/glove.6B.100d.txt.gz",
            "trainable": "false",
            "vocab_namespace": "lexicon_ids"
        },
        "multilabel": "false",
        "report_auxiliary_metrics": "true",
        "text_field_embedder": {
            "tokens": {
                "type": "embedding",
                "embedding_dim": 300,
                "pretrained_file": "/home/kanhon/Desktop/kanhon/NUS Computing/CS4248/github_dir/cs4248/scaffold/Project/sci-cite/scicite/pretrained_weights/GoogleNews-vectors-negative300.txt",
                "trainable": "false"
            }
        },
        "use_lexicon_features": "false",
        "use_sparse_lexicon_features": "false",
        "with_elmo": "true"
    },
    "train_data_path": "scicite_data/train.jsonl",
    "validation_data_path": "scicite_data/dev.jsonl",
    "test_data_path": "scicite_data/test.jsonl",
    "trainer": {
        "cuda_device": 0,
        "grad_clipping": 5,
        "num_epochs": 10,
        "optimizer": {
            "type": "adadelta",
            "rho": 0.95
        },
        "patience": 4,
        "validation_metric": "+average_F1"
    },
    "dataset_reader_aux": {
        "type": "scicite_section_title_data_reader",
        "with_elmo": "true"
    },
    "dataset_reader_aux2": {
        "type": "scicite_cite_worthiness_data_reader",
        "with_elmo": "true"
    },
    "evaluate_aux_on_test": "true",
    "evaluate_on_test": "true",
    "iterator_aux": {
        "type": "bucket",
        "batch_size": 16,
        "sorting_keys": [
            [
                "citation_text",
                "num_tokens"
            ]
        ]
    },
    "iterator_aux2": {
        "type": "bucket",
        "batch_size": 16,
        "sorting_keys": [
            [
                "citation_text",
                "num_tokens"
            ]
        ]
    },
    "mixing_ratio": "0.05",
    "mixing_ratio2": "0.05",
    "train_data_path_aux": "scicite_data/scaffolds/sections-scaffold-train.jsonl",
    "train_data_path_aux2": "scicite_data/scaffolds/cite-worthiness-scaffold-train.jsonl"
